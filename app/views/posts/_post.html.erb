<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Slug:</strong>
    <%= post.slug %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= post.body %>
  </p>

  <p>
    <strong>Body html:</strong>
    <%= post.body_html %>
  </p>

  <p>
    <strong>Public:</strong>
    <%= post.public %>
  </p>

  <p>
    <strong>Published at:</strong>
    <%= post.published_at %>
  </p>

  <p>
    <strong>Edited at:</strong>
    <%= post.edited_at %>
  </p>

</div>
